<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="CampusAppTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="FHBCampusAppTheme" parent="Theme.AppCompat.Light">
        <!-- No backgrounds, titles or window float -->
        <item name="android:displayOptions">useLogo</item>
    </style>

    <style name="FHB_TextAppearance_Mensa_Group">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/mensa_meal_tab_color_0</item>
    </style>


</resources>